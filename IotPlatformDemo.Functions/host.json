{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "durableTask": {
            "storageProvider": {
                "type": "AzureStorage"
            }
        },
        "serviceBus": {
            "maxConcurrentSessions": 100,
            "sessionIdleTimeout": "00:00:05"
        }
    }
}